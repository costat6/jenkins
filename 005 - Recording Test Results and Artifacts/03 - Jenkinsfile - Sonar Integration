pipeline {
    agent { 
		docker { 
			image 'maven:3.3.3' 
		}
	}
    stages {
        stage('tests') {
            steps {
                sh ""
            }
        }
		stage('SonarQube analysis') {
			steps {
                sh "mvn install \
					-f '005 - Recording Test Results and Artifacts/03 - Sonar Integration/'"
			}
		}
    }
}